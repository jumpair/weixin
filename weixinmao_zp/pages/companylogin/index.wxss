.container {
  background-color: #f5f5f5;
  min-height: 100%;
}
.container .header{
  width:750rpx;
  height: 350rpx;
  display: flex;
  flex-direction: column;
  align-items: center;

} 
 
.container .header .pic{

width:380rpx;
height: 250rpx;
align-self: center;
display: flex;
flex-direction: column;
align-items:center;

}
.container .header .pic image{
   width:140rpx;
   height: 140rpx;
   border:7rpx solid #FFF; 
   border-radius: 140rpx;
 
}
.container .header .pic view{

  height: 80rpx;
  line-height: 80rpx;
   width:380rpx;
   text-align: center;
   color: #FFF;
   font-size: .8rem;

}
.container .order{
  width:750rpx;
  display: flex;
  flex-direction: column;

}
.container .order .title{
  font-size: 1rem;
  padding-top:20rpx;
  padding-bottom: 20rpx;
  background: #FFF;
  margin-top:20rpx;
  padding-left: 20rpx;
  border-bottom: 1rpx solid #efefef;
}
.container .order .content{
    display: flex;
    flex-direction: row;
    justify-content: space-around;
    width:750rpx;
    background: #FFF;
    padding-bottom: 20rpx;
    padding-top: 20rpx;

}
.container .order .content .item{

  width:250rpx;
  text-align: center;
  font-size: .8rem;
  display: flex;
  flex-direction: column;
  align-items: center;

}
.container .order .content .item image{
    width:100rpx;
    height: 100rpx;

}
.container .list{

  width:750rpx;
  margin-top:20rpx;
  background: #FFF;
}




.container .list .item{
    padding-top: 25rpx;
    padding-bottom: 25rpx;
    font-size: .9rem;
    border-bottom: 1rpx solid #efefef;
    padding-left: 30rpx;
    color: #747474;
    display: flex;
    flex-direction: row;

}
.container .list .item image{
  width:50rpx;
  height: 50rpx;
  margin-right: 20rpx;

}



.line{

