page{

  background-color: #f8f8f9;


}
.container {
  background-color: #f8f8f9;
  min-height: 100%
}
.container .header{
  display: flex;
  flex-direction:column;  
  background:linear-gradient(to bottom,#ff6732,#ff4c35);
  width:750rpx;
  height: 450rpx;
  border-radius: 0 0 50% 50%;
  align-items: center;
}
.container .header .moneytitle{

  color: #ffed02;
  margin-top: 50rpx;

}

.container .header .money{

  color: #ffed02;
  font-weight: 600;
  font-size: 1.5rem;
  margin-top: 40rpx;
}

.container .title{
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 750rpx;  
}

.container .title .jobtitle{
  width:650rpx;
  height: 400rpx;
  background: #FFF;
  margin-top:-200rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
 
  padding-top:40rpx;
}
.container .title .jobtitle .jobname{
  font-weight: 600;
  font-size: 1.1rem;
}
.container .title .jobtitle .redtitle{
background: #fff9f0;
color: #ff9102;
font-size: .8rem;
margin-top:20rpx;
padding: 5rpx;

}

.container .title .jobtitle .jobdata{
  width:650rpx;
  display: flex;
  flex-direction: row;
  justify-content: baseline;
  margin-top:80rpx;
}

.container .title .jobtitle .jobdata .jobdataleft{
  display: flex;
  flex-direction: column;
  width:325rpx;
  align-items: center;

}

.container .title .jobtitle .jobdata .jobdataleft view:nth-child(1){

  color: #fd725c;
  font-size: .8rem;
  font-weight: bold;
}

.container .title .jobtitle .jobdata .jobdataleft view:nth-child(2){

  color: #999999;
  font-size: .8rem;
}


.container .title .jobtitle .jobdata .jobdataright{
  display: flex;
  flex-direction: column;
  width:325rpx;
  align-items: center;

}

.container .title .jobtitle .jobdata .jobdataright view:nth-child(2){

  color: #999999;
  font-size: .8rem;
}

.container .title .jobtitle .jobdata .jobdataright view:nth-child(1){

  color: #fd725c;
  font-size: .8rem;
  font-weight: bold;
}
.container .jobinfo{
  display: flex;
  flex-direction: column;
  align-items: center;
  background:#FFF;
  margin-top: 20rpx;
padding-top:  30rpx ;
padding-bottom:  10rpx ;
}

.container .jobinfo .jobtitle{
  width:650rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  
  
}
.container .jobinfo .jobtitle view:nth-child(1){
  font-weight: 600;
  font-size: 1rem;
}

.container .jobinfo .jobtitle view:nth-child(2){
 background: #fff9f0;
 color: #ff9102;
 font-size: .8rem;
 padding: 5rpx;
}


.container .jobinfo .joblabel{
  width:650rpx;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  margin-top:90rpx;
  
}

.container .jobinfo .joblabel view:nth-child(1){

  font-size:.8rem;
   color: #ff9102;
}
.container .jobinfo .joblabel view:nth-child(2){
  font-size: .8rem;
  color: #7b7a7a;
  
}
.container .jobinfo .joblabel view:nth-child(3){
  font-size: .8rem;
  color: #7b7a7a;
}


.container .jobdetail{
  background:#FFF;
  margin-top: 20rpx;
  display: flex;
  flex-direction: column;
  width:750rpx;
  align-items: center;
  border-bottom: 1rpx solid #eeeeee;

}

.container .jobdetail .jobtitle{
  font-weight: 600;
  font-size: 1rem;
  width:640rpx;
  padding-top:20rpx;
  padding-bottom: 20rpx;

}


.container .jobcontent{
  background:#FFF;
  display: flex;
  flex-direction: column;
  width:750rpx;
  align-items: center;

}

.container .jobcontent .detail{
  width:640rpx;
  padding-top:20rpx;
  padding-bottom: 20rpx;

}


