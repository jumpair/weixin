

/*充值金额分类*/
.balance-demo {
  padding: 40rpx 0 30rpx 0;

  background-color: #fff;
  display: flex;
  flex-direction: row;
  justify-content:space-between;
  width:750rpx;


}
.balance-demo button {
  display: inline-block;
  width: 300rpx;
  height: 120rpx;
  margin: 10rpx 35rpx;
  background-color: transparent;
}
.balance-demo button[data-id="5"] {
  padding-top: 30rpx;
}
.balance-demo button.selected {
  background-color: #3274e5;
  color: #fff;
}
.balance-demo button text {
  display: block;
  line-height: 60rpx;
}
.balance-demo button .botton {
  color: #3274e5;
}
.balance-demo button text.selected {
  color: #fff;
}
.other-button-hover {
  background-color: yellow;
}
/*<!--去充值模块-->*/
.goblance {
  width: 100%;
  box-sizing: border-box;
  margin-top: 80rpx;
  padding: 0 30rpx 0 30rpx;
  background-color: #eee;
  text-align: center;
}
.goblance view {
  color: #808080;
  background-color: #eee;
}
.goblance navigator {
  display: inline-block;
  color: #0033FF;
}
.goblance button[disabled="false"] {
  color: #fff;
}
.goblance button {
  color: #fff;
  margin: 20rpx 15rpx;
  background-color: #3274e5;
}

