  min-height: 100%
}
.container .header{
  display: flex;
  flex-direction:column;  
  background:linear-gradient(to bottom,#ff6732,#ff4c35);
  width:750rpx;
  height: 450rpx;
  border-radius: 0 0 50% 50%;
  align-items: center;
}
.container .header .moneytitle{

  color: #ffed02;
  margin-top: 50rpx;

}

