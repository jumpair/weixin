  align-items: center;
}
.news .newslist .title .detail .pic image{
  width:100rpx;
  height: 100rpx;
  align-self: center;
}
.news .newslist .title .detail .info{

    width:100%;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
}

.news .newslist .title .detail .info >  view:nth-child(1){
   padding-top: 10rpx;
