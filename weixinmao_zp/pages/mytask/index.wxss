/* search */
/**app.wxss**/
page {
	background-size: 100% 100%;
	height: 100%;
	font-size: 32rpx;
	font-family: "微软雅黑","方正黑体";
	color:#333;
  background: #f5f5f5;
}




.news{
margin-top:10rpx;
width:750rpx;
display: flex;
flex-direction: column;

}
.news .newstitle{
width:750rpx;
height: 80rpx;
line-height: 80rpx;
margin-left:20rpx;
font-size: .7rem;
border-bottom: 1rpx solid #efefef;
margin: 0 auto;
display: flex;
flex-direction: row;
align-content: center;

}

.news .newstitle view:nth-child(1){

  height: 40rpx;
  line-height: 40rpx;
  width:6rpx;
  background:#3274e5;
  align-self: center;
  margin-left: 10rpx;
  

}
.news .newstitle view:nth-child(2){

  height: 80rpx;
  line-height: 80rpx;
  width:150rpx;
  padding-left: 15rpx; 

}




 .news .newslist{
display: flex;
flex-direction: row;
width:720rpx;
margin: 0 auto;
border-bottom: 1px solid #efefef;
padding-top:10rpx;
padding-bottom:25rpx;
background: #FFF;
margin-top: 10rpx;
margin-bottom: 10rpx;
border-radius: 10rpx;
}



.news .newslist .title{
width:100%;
flex-direction: column;
display: flex;
justify-content: space-between;

}
.news .newslist .title .address{
  padding-left: 2%;
  font-size: .8rem;
  color: #646464; 
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  width:100%;
  padding-top: 20rpx;
}

.news .newslist .title .address view:nth-child(1){
   
    color:#000000;
    font-weight: bold;
    width: 68%;

}

.news .newslist .title .address view:nth-child(2){
   
    color:#ff4129;
    font-weight: bold;
    width: 30%;
    text-align: right;
    padding-right: 2%;
   

}

.news .newslist .title .detail{
  padding-left: 2%;
  text-align: left;
  font-size: .8rem;
  color: #000; 
  display: flex;
  flex-direction: row;
  width:100%;
}

.news .newslist .title .detail .pic {
  width:20%;
  display: flex;
  align-items: center;
}
.news .newslist .title .detail .pic image{
  width:100rpx;
  height: 100rpx;
  align-self: center;
}
.news .newslist .title .detail .info{

    width:100%;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
}

.news .newslist .title .detail .info >  view:nth-child(1){
   padding-top: 10rpx;
   color: #686868; 
   font-size: .7rem;
   padding-left: 2%;
   display: flex;
   flex-direction: column;
   width:65%;

    
}
.news .newslist .title .detail .info > view:nth-child(2){
   margin-top: 20rpx;
   padding-bottom: 10rpx;
   color: #999798; 
   font-size: .7rem;
   width:35%;
}


.news .newslist .title .detail .info .companyinfo view:nth-child(1){

  color: #666666; 

}
.news .newslist .title .detail .info .companyinfo view:nth-child(2){

    color: #979596; 
    margin-top: 20rpx;

}





.news .newslist .title .notedetail{
  padding-left: 2%;
  text-align: left;
  font-size: .8rem;
  color: #000; 
  display: flex;
  flex-direction: row;
  width:100%;
  margin-top:30rpx;
}

.news .newslist .title .notedetail .pic {
  width:20%;
  display: flex;
  align-items: center;
}
.news .newslist .title .notedetail .pic image{
  width:100rpx;
  height: 100rpx;
  align-self: center;
  border-radius: 50%;
}
.news .newslist .title .notedetail .info{

    width:100%;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
}

.news .newslist .title .notedetail .info >  view:nth-child(1){
   padding-top: 10rpx;
   color: #686868; 
   font-size: .7rem;
   padding-left: 2%;
   display: flex;
   flex-direction: column;
   width:65%;

    
}
.news .newslist .title .notedetail .info > view:nth-child(2){
   margin-top: 20rpx;
   padding-bottom: 10rpx;
   color: #999798; 
   font-size: .7rem;
   width:35%;
}


.news .newslist .title .notedetail .info .companyinfo view:nth-child(1){

  color: #666666; 
  font-weight: 600;
  font-size: .8rem;

}
.news .newslist .title .notedetail .info .companyinfo view:nth-child(2){

    color: #979596; 
    margin-top: 20rpx;

}







.news .newslist .title .price{

width:28%;
}

.news .newslist .title .price .money{
color:#d65408;
font-weight:bold;
font-size: .7rem;
padding-right: 10rpx;
}
.jobname{
  font-size: 1rem;
  color: #000000;
    display: flex;
  flex-direction: row;

}
.jobname>text{
color:#FFF;
background: #ff5d37;
padding: 6rpx;
border-radius: 5rpx;


}
.jobname image{

   width:40rpx;
  height: 40rpx;
  
}




.joblable{

  display: flex;
  flex-direction: row;
  justify-content: space-between;
  font-size: .8rem;
  color: #a6a6a6;
  padding-left: 2%;
} 

.vprice{
font-size: .7rem;
  background: #2de09f;
  font-weight: bold;
  color: #FFF;
  height: 60rpx;
  line-height: 60rpx;
  width:180rpx;
  text-align: center;
  border-radius: 5rpx;
}

.special{

  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  flex-wrap: wrap;
    font-size: .8rem;
  padding-left: 2%;
}
.special .item{

  margin-right: 10rpx;
  border-radius: 5rpx;
  padding: 10rpx;
  margin-top: 10rpx;
  
}

.special > .item:nth-child(2n-1){

 background: #e4fcdc;
  color:#76af58;
}
.special > .item:nth-child(2n){

 background: #fef5da;
  color:#ea994e;
}
.special > .item:nth-child(4n-1){

 background: #daf2fe;
  color:#30a2c8;
}

.jobmoney{
  display: flex;
  flex-direction: column;
  align-items: right;
}


