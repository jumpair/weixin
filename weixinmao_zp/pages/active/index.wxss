/* search */
/**app.wxss**/
page {
	background-size: 100% 100%;
	height: 100%;
	font-size: 32rpx;
	font-family: "微软雅黑","方正黑体";
	color:#333;
}

.colorred {
	color: #e62129;
}

.city_header {
	display: flex;
	width: 92%;
	line-height: 50px;
	padding: 0 4%;
	border-bottom: 1px solid #f5f5f5;
	position: fixed;
	top: 0;
	z-index:99;
	background-color: #fff;
}

.city_header image {
	width: 18px;
	height: 18px;
	padding-top: 17px;
}

.city_header text {
	flex: 1;
	color: #333;
	font-size: 16px;
	font-weight: bold;
	text-align: center;
}

/* 列表 */
.carlist {
	display: flex;
	flex-direction: column;
	width: 92%;
	padding: 5px 4%;
	border-bottom: 1px solid #ececec;
}

.carlist:last-child {
	border-bottom: 0;
}

.carlist_top {
	display: flex;
}

.car_imgs {
	position: relative;
	width: 104px;
	height: 78px;
}

.newtop {
	position: absolute;
	top: 0;
	left: 0;
	padding: 1px 3px;
	text-align: center;
	background-color: #ff3a3b;
	font-size: 10px;
  color: #fff;
}

.picnum {
	position: absolute;
	bottom: 5px;
	right: 5px;
	padding: 0 5px;
	font-size: 10px;
	color: #fff;
	text-align: center;
	background-color: rgba(0,0,0,0.4);
	border-radius: 4px;
}

.car_imgs image {
	width: 100%;
	height: 100%;
}

.car_info {
	flex: 1;
	width: 60%;
	padding-left: 5px;
}

.car_name {
	display: block;
	position: relative;
	line-height: 20px;
	height: auto;
	overflow: hidden;
	color: #333;
	font-size: 15px;
	text-overflow:ellipsis;
	white-space:nowrap;
}

.car_hid {
	display: block;
	line-height: 20px;
	padding-top: 15px;
	color: #666;
	font-size: 12px;
	overflow: hidden;
	text-overflow:ellipsis;
	white-space:nowrap;
}

.car_present {
	display: flex;
}

.current_price {
	line-height: 20px;
	font-size: 15px;
	color: #db0000;
}

.original_price {
	color: #929292;
	font-size: 12px;
	text-decoration: line-through;
	padding-left: 5px;
}

.car_present {
	padding-top: 5px;
}

.car_tag {
	flex: 1;
	text-align: right;
}

.car_tag text {
	padding: 0 3px;
	color: #ff7e00;
	border: 1px solid #ff7e00;
	border-radius: 2px;
	font-size: 12px;
	line-height: 14px;
}

.carlist_buttom {
	display: flex;
	flex: 1;
	padding-top: 5px;
}

.carlist_buttom text {
	font-size: 12px;
}

.owner_sm {
	line-height: 14px;
	padding: 0 3px;
	margin-right: 5px;
	border: 1px solid #e62129;
	border-radius: 2px;
	color: #e62129;
}

.owner_rz {
	line-height: 14px;
	padding: 0 3px;
	margin-right: 5px;
	background-color: #e62129;
	border-radius: 2px;
	color: #fff;
}

.owner_time {
	flex: 1;
	text-align: right;
}

/* 侧栏字母 */
.aside_letter {
	position: fixed;
	top: 58px;
	right: 0;
	z-index: 66;
}

.aside_letter text {
	display: block;
	width: 30px;
	font-size: 12px;
	line-height: 20px;
	color: #e62129;
	text-align: center;
}

/* 排序选中 */
.select_cars_list {
	display: flex;
	border-bottom: 1px solid #ececec;
}

.select_cars_list image {
	width: 18px;
	height: 18px;
	padding-top: 8px;
}

/* 售出 */
.sold {
  background: url(http://s2.nongjigou.com/images/sold.png) no-repeat right 0;
  background-size: 70px 70px;
}
.search {
	display: flex;
	width: 92%;
	height: 50px;
	padding: 0 4%;
	border-bottom: 1px solid #ececec;
	top: 0;
	z-index: 99;
	background-color: #fff;
}

navigator {
	line-height: 50px;
}

.search image {
	width: 30rpx;
	height: 34rpx;
	padding-right: 3px;
	vertical-align: sub;
}

.search text {
	line-height: 30px;
	font-size: 14px;
	color: #e62129;
}

.search_box {
	display: flex;
	flex: 1;
	height: 30px;
	padding-left: 15px;
	margin: 10px 0 0 10px;
	background-color: #f7f7f7;
}

.search_box icon {
	padding-top: 8px;
}

.search_box text {
	flex: 1;
	padding-left: 5px;
	color: #999;
}

