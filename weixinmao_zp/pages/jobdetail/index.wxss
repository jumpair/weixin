page{

  background-color: #f8f8f9;


}
.container {
  background-color: #f8f8f9;
  min-height: 100%
}
.container .header{
  display: flex;
  flex-direction:column;  
  background:linear-gradient(to bottom,#ff6732,#ff4c35);
  width:750rpx;
  height: 450rpx;
  border-radius: 0 0 50% 50%;
  align-items: center;
}
.container .header .moneytitle{

  color: #ffed02;
  margin-top: 50rpx;

}

.container .header .money{

  color: #ffed02;
  font-weight: 600;
  font-size: 1.5rem;
  margin-top: 40rpx;
}

.container .title{
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 750rpx;  
}

.container .title .jobtitle{
  width:730rpx;
  height: 400rpx;
  background: #FFF;
  margin-top:-200rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
 
  padding-top:40rpx;
}
.container .title .jobtitle .jobname{
  font-weight: 600;
  font-size: 1.1rem;
}
.container .title .jobtitle .redtitle{
background: #fff9f0;
color: #ff9102;
font-size: .8rem;
margin-top:20rpx;
padding: 5rpx;

}

.container .title .jobtitle .jobdata{
  width:730rpx;
  display: flex;
  flex-direction: row;
  justify-content: baseline;
  margin-top:80rpx;
}

.container .title .jobtitle .jobdata .jobdataleft{
  display: flex;
  flex-direction: column;
  width:325rpx;
  align-items: center;

}

.container .title .jobtitle .jobdata .jobdataleft view:nth-child(1){

  color: #fd725c;
  font-size: .8rem;
  font-weight: bold;
}

.container .title .jobtitle .jobdata .jobdataleft view:nth-child(2){

  color: #999999;
  font-size: .8rem;
}


.container .title .jobtitle .jobdata .jobdataright{
  display: flex;
  flex-direction: column;
  width:325rpx;
  align-items: center;

}

.container .title .jobtitle .jobdata .jobdataright view:nth-child(2){

  color: #999999;
  font-size: .8rem;
}

.container .title .jobtitle .jobdata .jobdataright view:nth-child(1){

  color: #fd725c;
  font-size: .8rem;
  font-weight: bold;
}
.container .jobinfo{
  display: flex;
  flex-direction: column;
  align-items: center;
  background:#FFF;
  margin-top: 20rpx;
padding-top:  30rpx ;
padding-bottom:  10rpx ;
}

.container .jobinfo .jobtitle{
  width:730rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  
  
}
.container .jobinfo .jobtitle view:nth-child(1){
  font-weight: bold;
  font-size: 1.2rem;
}

.container .jobinfo .jobtitle view:nth-child(2){
 background: #fff9f0;
 color: #ff9102;
 font-size: 1rem;
 padding: 5rpx;
   font-weight: bold;

}


.container .jobinfo .joblabel{
  width:730rpx;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  flex-wrap: wrap;
  margin-top:20rpx;
  font-size:.7rem;
  color: #7b7a7a;
  
}



.advmoney{
  width:750rpx;
}


.container .jobinfo .special {
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  flex-wrap: wrap;
  margin-top: 30rpx;
  font-size: .8rem;
  width:730rpx;


}
.container .jobinfo .special .item{

  padding-left: 15rpx;
  padding-right: 15rpx;
  padding-top: 10rpx;
  padding-bottom: 10rpx;
 
  border-radius: 5rpx;
  margin-right: 10rpx;
  margin-bottom: 15rpx;
}
.container .jobinfo .special .item:nth-child(2n-1){

 background: #e4fcdc;
  color:#76af58;
}
.container .jobinfo .special .item:nth-child(2n){

 background: #fef5da;
  color:#ea994e;
}
.container .jobinfo .special .item:nth-child(4n-1){

 background: #daf2fe;
  color:#30a2c8;
}







.container .joblabel view{

  padding-left: 15rpx;
  padding-right: 15rpx;
  padding-top: 10rpx;
  padding-bottom: 10rpx;
 
  border-radius: 5rpx;
  margin-right: 10rpx;
  margin-bottom: 15rpx;
}
.container .joblabel view:nth-child(2n-1){

 background: #e4fcdc;
