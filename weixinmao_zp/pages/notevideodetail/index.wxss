/* weixinmao_hssy/pages/user/index.wxss */
.container {
  background-color: #FFF;
  min-height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.container .header{
  width:730rpx;

  padding-top:10rpx;
 
}


.container .order{
  width:720rpx;
  display: flex;
  flex-direction: column;


}
.container .order .title{
  font-size:.8rem;
  padding-top:20rpx;
  padding-bottom: 20rpx;
  background: #FFF;
  color:#666666;
  margin-top:20rpx;
  padding-left: 20rpx;
  border-bottom: 1rpx solid #efefef;
  font-weight: 600;
}
.container .order .content{
    display: flex;
 
    background: #FFF;
    padding: 20rpx;
    text-indent: 20rpx;
    font-size: .8rem;
     color:#666666;
     padding-bottom: 120rpx;
    

}
