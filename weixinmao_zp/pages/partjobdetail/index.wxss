/* weixinmao_house/pages/newhousedetail/index.wxss */
.container {
  background-color: #e5e5e5;
  min-height: 100%
}



.container .header{
  display: flex;
  width: 750rpx;
  flex-direction: column;
  align-items: center;
  background: #FFF;
}
.container .header>image{
width:750rpx;
height: 350rpx;
}
.container .main{
  display: flex;
  width:750rpx;
  flex-direction:column;
  padding-bottom: 20rpx;
  background: #FFF;

}
.container .main .title{
  display: flex;
  width:700rpx;
  flex-direction:column;
  padding-top: 20rpx;
  font-size: .9rem;
  padding-left: 30rpx;

}
.container .main .title view{


}
.container .main .title view text{
color:#1aac19;

}
.container .main .title view:nth-child(1){
  font-size: 1.1rem;

}
.container .main .title .price{
  display: flex;
  flex-direction: row;
}
.container .main .title .price view:nth-child(1){

  font-size: .9rem;
  color: #d51a39;

}

.container .main .title .price view:nth-child(2){

  font-size: .7rem;
  color: #FFF;
  background: #ff706a;
  border-radius: 4rpx;
  padding: 5rpx 20rpx;
  margin-left: 100rpx;

}
.container .main .title .price view:nth-child(3){

  font-size: .7rem;
  color: #FFF;
  background: #ff706a;
  border-radius: 4rpx;
  padding: 5rpx 20rpx;
  margin-left: 50rpx;

}
.container .main .address{
  display: flex;
  flex-direction: column;
  padding-left: 30rpx;
  font-size: .7rem;
  margin-top: 10rpx;
  color: #777676;
  border-bottom: 1rpx solid #e5e5e5;

}
.container .main .address .money{
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  flex-wrap: wrap;


}
.container .main .address .money .item{

  padding-left: 15rpx;
  padding-right: 15rpx;
  padding-top: 10rpx;
  padding-bottom: 10rpx;
 
  border-radius: 40rpx;
  margin-right: 10rpx;
  margin-bottom: 15rpx;
}
.container .main .address .money .item:nth-child(2n-1){

 background: #e4fcdc;
  color:#76af58;
}
.container .main .address .money .item:nth-child(2n){

 background: #fef5da;
  color:#ea994e;
}
.container .main .address .money .item:nth-child(4n-1){

 background: #daf2fe;
  color:#30a2c8;
}
.container .main .jobintro{
  display: flex;
  flex-direction: row;
  padding-left: 30rpx;
  font-size: .7rem;
  margin-top: 20rpx;

  color: #777676;
  justify-content: space-around;
  flex-wrap: wrap;

}

.container .main .jobintro view{
 width:30%;
   margin-bottom: 20rpx;
}
.container .main .jobintro view text{
  margin-right: 10rpx;
}
.container .companyinfo{
margin-top:10rpx;
width:750rpx;
background: #FFF;
display: flex;
flex-direction: row;
justify-content: center;
border-bottom: 1rpx solid #e5e5e5;


}
.container .companyinfo image{

  width:150rpx;
  height: 150rpx;
  align-self: center;
  
}
.company{

  background: #FFF;
  display:flex;
  flex-direction: column;
  margin-top: 10rpx;

}
.container .companyinfo .companyname{
  width:70%;
  display: flex;
  flex-direction: column;
  margin-left: 5%;
   padding-top:40rpx;
  padding-bottom:40rpx;
 
}
.container .companyinfo .companyname view:nth-child(1){

  font-size: .8rem;
  color: #000;
}
.container .companyinfo .companyname view:nth-child(2){

 font-size: .7rem;
  color: #777676;
}
.container .companyinfo .companyname view:nth-child(3){
 font-size: .7rem;
  color: #777676;

}
.container .companyaddress{
      padding-left: 20rpx;
       padding-top: 20rpx;
        padding-bottom: 20rpx;
      font-size: .8rem;
}

.container .contentlist{
margin-top:10rpx;
width:750rpx;
background: #FFF;
display: flex;
flex-direction: column;

}
.container .contentlist .title{
width:750rpx;
height: 80rpx;
line-height: 80rpx;
font-size: .9rem;
border-bottom: 1rpx solid #efefef;
display: flex;
flex-direction: row;
align-content: center;

}



.container .contentlist .title view:nth-child(1){

  height: 40rpx;
  line-height: 40rpx;
  width:6rpx;
  background:#3274e5;
  align-self: center;
  

}
.container .contentlist .title view:nth-child(2){

  height: 80rpx;
  line-height: 80rpx;
  width:200rpx;
  padding-left: 15rpx; 

}
.container .contentlist .listitem{
width:750rpx;
display: flex;
flex-direction: column;

}
.container .contentlist .listitem .items{
 padding-left:30rpx;
 color:#777676;
 height: 80rpx;
 line-height: 80rpx;
 font-size: .8rem;
 border-bottom: 1rpx dotted #e5e5e5;
 display: flex;
 flex-direction: row;
 justify-content: space-between;
}
.container .contentlist .listitem .items text{

  color:#000;
  padding-left:10rpx;
}
.copytel{
  background: #fe552e;
  border:0;
  width:200rpx;
  height: 60rpx;
  line-height: 60rpx;
  font-size: .7rem;
  color: #FFF;
  padding-right: 10rpx;
  margin-top:20rpx;
  

}

.type-container{
  width: 100%;

  background-color: #fff;
  overflow: hidden;

}

.type-navbar{
  white-space: nowrap;
  

  background-color: #fff;
}
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}
.type-box{
  padding-top:10rpx;
  width:250rpx;
  box-sizing: content-box;
  font-size: 24rpx;
  text-align: center;
  display: inline-block;
  overflow: hidden;
}
.type-box image{
  width:230rpx;
  height:230rpx; 
}
.type-navbar-item{
  border-bottom: 4rpx solid #fff;
}
.type-item-on{
  border-bottom: 4rpx solid #e64340;
}
